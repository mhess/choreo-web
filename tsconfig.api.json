{
	"include": ["./api/*.ts"],
	"compilerOptions": {
		"target": "es2022",
		"allowJs": true,
		"skipLibCheck": true,
		"strict": true,
		"noEmit": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"incremental": true,
		"baseUrl": "./api"
	}
}
